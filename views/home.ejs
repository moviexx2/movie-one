<table
    id="example"
    class="table table-striped table-bordered"
    style="width: 100%;"
>
    <form action="/movies/findByUserID/?" method="GET">
        <input type="text" name="UserID" id="UserID" />
    </form>
    <thead>
        <tr>
            <th>No</th>
            <th>Movie Name</th>
            <th>Id Movie</th>
            <th>Genre</th>
            <th>Years</th>
            <th>Created By</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i= 0; i < result.length; i++) { %>
        <tr>
            <td><%= i+1; %></td>
            <td><%= result[i].name %></td>
            <td><%= result[i]._id %></td>
            <td><%= result[i].genre %></td>
            <td><%= result[i].years %></td>
            <!-- <td><%= result[i].UserID %></td> -->
            <td>
                <button class="btn btn-primary">Edit</button>
                <button class="btn btn-danger">Delete</button>
            </td>
        </tr>
        <% } %>
    </tbody>
</table>
